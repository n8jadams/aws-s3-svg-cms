{
  "name": "aws-s3-svg-cms",
  "version": "1.0.0",
  "private": true,
  "description": "A CMS for managing SVGs and generating an SVG sprite sheet",
  "keywords": [],
  "license": "MIT",
  "author": "Nate Adams",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --production",
    "build:server": "npm run copy-env && ttsc --project server/tsconfig.json",
    "copy-env": "cpy '../.env' ../dist/server/ --cwd=server/ --parents",
    "dev": "concurrently -p \"[{name}]\" -n \"server,client\" -c \"bgGreen.bold,bgCyan.bold\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "webpack --watch",
    "dev:server": "npm run copy-env && tsc-watch --noClear --compiler ttypescript/bin/tsc -p server/tsconfig.json --outDir dist/server --onCompilationComplete \"node .dev/alias.js\" --onSuccess \"node --inspect dist/server/server.js\" || exit 0",
    "preinstall": "npx npm-force-resolutions",
    "start": "node dist/server/server.js"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "elliptic": ">=6.5.3",
    "lodash": "^4.17.19",
    "minimist": "^1.2.3",
    "node-fetch": "^2.6.1",
    "yargs-parser": "13.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/node": "^13.13.15",
    "@types/react": "^16.9.44",
    "aws-sdk": "^2.726.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.2.0",
    "cpy-cli": "^3.1.0",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.1.1",
    "fastify-static": "^4.2.4",
    "handlebars": "^4.7.7",
    "html-webpack-plugin": "^4.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsdom": "^16.3.0",
    "point-of-view": "^4.3.0",
    "purgecss-webpack-plugin": "^2.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.2",
    "react-top-loading-bar": "^1.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "svg-sprite": "^1.5.0",
    "terser-webpack-plugin": "^2.3.7",
    "tsc-watch": "^4.2.9",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.7",
    "typescript-is": "^0.15.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
