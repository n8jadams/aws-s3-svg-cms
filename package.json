{
  "name": "aws-s3-svg-cms",
  "version": "1.0.0",
  "private": true,
  "description": "A CMS for managing SVGs and generating an SVG sprite sheet",
  "keywords": [],
  "license": "MIT",
  "author": "Nate Adams",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --production",
    "build:server": "npm run copy-env && ttsc --project server/tsconfig.json",
    "copy-env": "cpy '../.env' ../dist/server/ --cwd=server/ --parents",
    "dev": "concurrently -p \"[{name}]\" -n \"server,client\" -c \"bgGreen.bold,bgCyan.bold\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "webpack --watch",
    "dev:server": "npm run copy-env && tsc-watch --noClear --compiler ttypescript/bin/tsc -p server/tsconfig.json --outDir dist/server --onSuccess \"npm run start\" || exit 0",
    "preinstall": "npx npm-force-resolutions",
    "start": "node dist/server/server.js"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "minimist": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.27",
    "aws-sdk": "^2.656.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "cpy-cli": "^3.1.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "fastify": "^2.13.0",
    "fastify-static": "^2.6.0",
    "handlebars": "^4.7.6",
    "html-webpack-plugin": "^4.0.4",
    "isomorphic-unfetch": "^3.0.0",
    "jsdom": "^16.2.2",
    "point-of-view": "^3.8.0",
    "purgecss-webpack-plugin": "^2.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-dropzone": "^10.2.2",
    "react-top-loading-bar": "^1.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "svg-sprite": "^1.5.0",
    "terser-webpack-plugin": "^2.3.5",
    "tsc-watch": "^4.2.3",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-is": "^0.15.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
